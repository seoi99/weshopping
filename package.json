{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"DEBUG=app,app:* nodemon app.js\"  \"npm run frontend\"",
    "lint": "eslint app.js",
    "backend": "DEBUG=app,app:* nodemon app.js",
    "frontend-install": "npm install --prefix client",
    "frontend": "npm start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "chalk": "1.1.3",
    "cheerio": "^1.0.0-rc.3",
    "clean": "^4.0.2",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "debug": "^2.6.6",
    "ejs": "^2.6.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "4.15.2",
    "express-session": "^1.15.6",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "~3.0.8",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.2.1",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^1.17.0",
    "request": "^2.88.0",
    "validator": "^11.1.0",
    "xml2js": "^0.4.19"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "PORT": "4000",
      "NODE_ENV": "development"
    }
  }
}
